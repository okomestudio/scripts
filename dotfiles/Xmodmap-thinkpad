clear lock
clear control
clear mod4

!CAPS lock key
keycode 66 = Control_L

!Meta (R) key of HHKB
keycode 100 = Control_R

!Meta (L) key of HHKB
keycode 102 = Super_L

!Ins key
keycode 118 = Caps_Lock

!Menu key of MS Sculpt Ergonomic
!keycode 135 = Control_R

! For ThinkPad keyboard:
keycode 107 = Alt_R Print

add mod4 = Super_L Super_R Hyper_L
add control = Control_L Control_R
add lock = Caps_Lock
